<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.springlec.base.dao.QnaContentDao">
	<select id="contentDao" resultType="com.springlec.base.model.QnaContentDto">
		SELECT qnaSeq, qnaTitle, qnaContent, qnaDate 
		FROM qna
		WHERE qnaSeq = #{qnaSeq}
	</select>
</mapper>